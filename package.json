{
  "name": "slidev-theme-nmt",
  "version": "0.9.2",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.49.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "latest",
    "slidev-addon-sync": "latest",
    "slidev-component-poll": "latest"
  },
  "devDependencies": {
    "@iconify-json/fa7-brands": "latest",
    "@iconify-json/fa7-regular": "latest",
    "@iconify-json/fa7-solid": "latest",
    "@slidev/cli": "latest",
    "@unocss/preset-icons": "latest",
    "playwright-chromium": "latest",
    "unocss": "latest"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "dark",
    "defaults": {
      "fonts": {
        "mono": "Fira Code",
        "serif": "Roboto Serif",
        "sans": "Lexend,Roboto"
      },
      "transition": "fade",
      "aspectRatio": "16/9"
    }
  },
  "author": "Adrian Gould <ady.g.coder@gmail.com>",
  "funding": "https://github.com/sponsors/adygcode",
  "bugs": "https://github.com/adygcode/slidev-theme-nmt/issues"
}
