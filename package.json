{
  "name": "slidev-theme-nmt",
  "version": "0.5.1",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.49.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@iconify-json/fa7-brands": "^1.2.1",
    "@iconify-json/fa7-solid": "^1.2.3",
    "@slidev/types": "^52.11.3",
    "slidev-addon-sync": "^0.1.0",
    "slidev-component-poll": "^3.0.0"
  },
  "devDependencies": {
    "@slidev/cli": "^52.11.3",
    "playwright-chromium": "^1.58.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "dark",
    "defaults": {
      "fonts": {
        "mono": "Fira Code",
        "serif": "Roboto Serif",
        "sans": "Lexend,Roboto"
      },
      "transition": "fade",
      "aspectRatio": "16/9"
    }
  },
  "author": "Adrian Gould <ady.g.coder@gmail.com>",
  "funding": "https://github.com/sponsors/adygcode",
  "bugs": "https://github.com/adygcode/slidev-theme-nmt/issues"
}
